{
  "name": "apollo-cli",
  "description": "CLI for managing an Apollo Collaboration Server",
  "version": "0.0.0",
  "author": "Apollo Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/Apollo3.git",
    "directory": "packages/apollo-cli"
  },
  "bin": {
    "apollo": "./bin/run.js"
  },
  "type": "module",
  "main": "",
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "tsx bin/dev.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha 'src/**/*.test.ts'",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^3.14.1",
    "@oclif/plugin-help": "^6.0.8",
    "axios": "^1.6.2",
    "open": "^9.1.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@oclif/test": "^3.1.3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18.14.2",
    "mocha": "^10.2.0",
    "oclif": "^4.1.0",
    "shx": "^0.3.3",
    "ts-node": "^10.3.0",
    "tsx": "^4.6.2",
    "typescript": "^5.1.6"
  },
  "oclif": {
    "bin": "apollo",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  }
}
