    "babel-plugin-polyfill-regenerator": "^0.6.6",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "builtin-modules": "^5.0.0",
    "chalk": "^5.6.2",
    "cypress": "12.17.3",
    "cypress-image-diff-html-report": "^2.2.0",
    "cypress-image-diff-js": "^2.3.0",
